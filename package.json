{
  "name": "powerBI-mapboxgl",
  "version": "0.0.1",
  "description": "PowerBI visualization with mapboxgl",
  "main": "index.js",
  "scripts": {
    "pbi": "pbiviz",
    "pbicerti": "pbiviz --install-cert",
    "package": "pbiviz package",
    "pretest": "pbiviz package --resources --no-minify --no-pbiviz --no-plugin",
    "test": "karma start",
    "start": "pbiviz start",
    "turf": "browserify ./turf-build.js -s turf --debug --plugin [minifyify --output src/js/turf-bundle-min.js] > src/js/turf-bundle-min.js"
  },
  "author": "Ryan Baumann",
  "license": "MIT",
  "dependencies": {
    "@mapbox/mapbox-gl-geocoder": "^2.2.0",
    "@mapbox/mapbox-gl-supported": "^1.3.0",
    "@turf/bbox": "^6.0.1",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/helpers": "^6.1.3",
    "@turf/meta": "^6.0.1",
    "@types/d3": "^4.12.0",
    "@types/geojson": "^1.0.6",
    "@types/mapbox-gl": "^0.41.0",
    "babel-preset-es2015": "^6.24.1",
    "chroma-js": "^1.3.4",
    "detect-browser": "^2.0.0",
    "mapbox": "^1.0.0-beta9",
    "mapbox-gl": "^0.45.0-beta.1",
    "mapbox-gl-supported": "^1.2.0",
    "mbview": "^1.15.2",
    "powerbi-client": "^2.3.4",
    "powerbi-visuals-tools": "^1.10.1",
    "powerbi-visuals-utils-dataviewutils": "^1.2.0",
    "powerbi-visuals-utils-testutils": "^1.1.0",
    "powerbi-visuals-utils-typeutils": "^1.1.0",
    "typed-rest-client": "^1.0.5",
    "typescript": "^2.7.2",
    "uglify-js": "^3.3.14"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.4",
    "@types/jasmine-jquery": "^1.5.32",
    "jasmine-core": "^2.8.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.1",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript-preprocessor": "^0.3.1",
    "minifyify": "^7.3.5"
  }
}
