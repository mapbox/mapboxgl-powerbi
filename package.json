{
  "name": "mapboxgl-powerbi",
  "version": "0.1.1",
  "description": "Mapbox Visual for Power BI",
  "main": "index.js",
  "scripts": {
    "pbi": "pbiviz",
    "pbicert": "pbiviz --create-cert",
    "pbicerti": "pbiviz --install-cert",
    "package": "pbiviz package --no-minify",
    "pretest": "pbiviz package --resources --no-minify --no-pbiviz --no-plugin",
    "test": "karma start",
    "start": "pbiviz start",
    "turf-build": "browserify ./src/build/turf-build.js -s turf --debug --plugin [minifyify --output src/js/turf-bundle-min.js] > src/js/turf-bundle-min.js"
  },
  "author": "Ryan Baumann",
  "license": "MIT",
  "dependencies": {
    "@turf/bbox": "^6.0.1",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/center": "^6.0.1",
    "@turf/helpers": "^6.1.3",
    "@turf/meta": "^6.0.1",
    "@types/geojson": "^1.0.6",
    "@types/mapbox-gl": "^0.44.3",
    "chroma-js": "^1.3.4",
    "mapbox-gl": "^0.45.0",
    "powerbi-client": "^2.3.4",
    "powerbi-visuals-tools": "^1.10.1",
    "powerbi-visuals-utils-dataviewutils": "^1.2.0",
    "powerbi-visuals-utils-testutils": "^1.1.0",
    "powerbi-visuals-utils-typeutils": "^1.1.0",
    "supercluster": "^3.0.2",
    "typed-rest-client": "^1.0.5"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.4",
    "@types/jasmine-jquery": "^1.5.32",
    "jasmine-core": "^2.8.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.1",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript-preprocessor": "^0.3.1",
    "minifyify": "^7.3.5"
  }
}
