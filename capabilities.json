        "geography": {
          "longitude": true
        }
      }],
      "requiredTypes": [{
          "numeric": true
        },
        {
          "integer": true
        },
        {
          "geography": {
            "longitude": true
          }
        }
      ]
    },
    {
      "name": "color",
      "kind": "Measure",
      "displayName": "Color",
      "description": "Measure or Dimension to color circles and heatmaps.  Can be numeric or text data type.",
      "requiredTypes": [{
          "numeric": true
        },
        {
          "integer": true
        },
        {
          "text": true
        },
        {
          "bool": true
        }
      ]
    },
    {
      "name": "size",
      "kind": "Measure",
      "displayName": "Size",
      "description": "Measure to control circle radius size. Must be numeric data type.",
      "requiredTypes": [{
          "numeric": true
        },
        {
          "integer": true
        }
      ]
    },
    {
      "name": "cluster",
      "kind": "Measure",
      "displayName": "Cluster",
      "description": "Measure to aggregate points for clustering.  Must be numeric data type.",
      "requiredTypes": [{
          "numeric": true
        },
        {
          "integer": true
        }
      ]
    },
    {
      "name": "tooltips",
      "kind": "Measure",
      "displayName": "Tooltips",
      "description": "Values to display in tooltips"
    }
  ],
  "dataViewMappings": [{
    "conditions": [{
      "location": {
        "max": 1
      },
      "latitude": {
        "max": 1
      },
      "longitude": {
        "max": 1
      },
      "color": {
        "max": 1
      },
      "size": {
        "max": 1
      },
      "cluster": {
        "max": 1
      }
    }],
